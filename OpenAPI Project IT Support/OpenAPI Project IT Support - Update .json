          },
          {
            "name": "MaxPriceOthers",
            "description": "menginput harga lain-lainnya, dari project tersebut",
            "required": true,
            "in": "query",
            "schema": {
              "type": "string",
              "nullable": false,
              "maxLength": 50,
              "minLength": 1
            }
